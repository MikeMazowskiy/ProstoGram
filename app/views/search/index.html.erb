<div class="search-page">
  <% if @posts.exists? %>
    <h2>Posts from</h2>
  <% else %>
    <h2> No matching posts</h2>
    <% end %>


<div class="user-images">
    <% @posts.each do |post| %>
      <div class="wrapper">
          <%=image_tag post.image %>
      </div>
    <% end %>
  </div>
</div>